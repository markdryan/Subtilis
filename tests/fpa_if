LOCAL a
LET a = 3.14
LET b = 17.6666
LET c = 3.14
PRINT a < b
PRINT a > b
PRINT a = b
PRINT a = c
PRINT a <> b
PRINT a <> c
PRINT a < 17.6666
PRINT a > 17.6666
PRINT a = 17.6666
PRINT a = 3.14
PRINT a <> 17.6666
PRINT a <> 3.14
PRINT 3.14 < b
PRINT 3.14 > b
PRINT 3.14 = b
PRINT 3.14 = c
PRINT 3.14 <> b
PRINT 3.14 <> c
PRINT a <> 2.0
PROCLogical

DEF PROCLogical
LOCAL a
LET a = 3.14
PRINT a OR b
PRINT a AND b
PRINT a EOR b
PRINT NOT a
PRINT a OR 17.6666
PRINT a AND 17.6666
PRINT a EOR 17.6666
PRINT NOT 3.14
ENDPROC
