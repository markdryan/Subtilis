LOCAL a
LET a = 3.14
LET B% = 11

IF B% > a THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

IF 10 < a THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

IF 10 <= a THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

IF 11 = a THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

IF 3.14 = a THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

IF 3.14 <> a THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

IF 3.14 >= a THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

PROCTestVars(a, 11.0)

DEF PROCTestVars(a,b)

IF a < b THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

IF a <= a THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

IF a > b THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

IF b >= b THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

IF a = b THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

IF a <> b THEN
PRINT TRUE
ELSE
PRINT FALSE
ENDIF

ENDPROC

