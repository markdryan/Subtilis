PROCDIVByZero
PROCDivideByZero
PROCRDivideByZero

DEF PROCDIVByZero
    LOCAL A%
    LOCAL B%

    A% = 100
    B% = 0

    ONERROR
        PRINT ERR
        ENDPROC
    ENDERROR

    PRINT A% DIV B%
    PRINT &ff
ENDPROC

DEF PROCDivideByZero
    LOCAL A
    LOCAL B

    A = 100
    B = 0

    ONERROR
        PRINT ERR
        ENDPROC
    ENDERROR

    PRINT A / B
    PRINT &ff
ENDPROC

DEF PROCRDivideByZero
    LOCAL A
    LOCAL B

    A = 100
    B = 0

    ONERROR
        PRINT ERR
        ENDPROC
    ENDERROR

    PRINT 10 / B
    PRINT &ff
ENDPROC

